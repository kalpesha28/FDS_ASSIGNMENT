def createlist(X, ab_students, n):
    for i in range(n):
        marks = input("Enter marks scored: ")
        if marks == "ab":
            ab_students.append(i)  
        else:
            X.append(int(marks)) 

X = []
ab_students = [] 
n = int(input("Enter the number of students: "))
createlist(X, ab_students, n)
print("Marks:", X)

def avg():
        sum = 0
        for i in X:
            sum += i
        average = sum / len(X)
        print("Average marks:", average)
    
avg()

def maximum():
    if len(X) > 0:
        max_val = X[0]
        for i in X:
            if i > max_val:
                max_val = i
        print(max_val, "is the highest")
    
maximum()

def minimum():
    if len(X) > 0:
        min_val = X[0]
        for i in X:
            if i < min_val:
                min_val = i
        print(min_val, "is the lowest")
   
minimum()

def absent(X, ab_students):
    count = len(ab_students)  
    print("Absent students are:", count)

absent(X, ab_students)
   
  
   
def frequency():
  max = 0
  res = X[0]
  for i in X:
    freq = X.count(i)
    if freq > max:
        max = freq
        res = i
  print("highest frequency=",res)      
frequency()        
